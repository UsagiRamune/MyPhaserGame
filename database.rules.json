{
  "rules": {
    "leaderboard": {
      ".read": "true",

      "$uid": {
        ".write": "auth != null && auth.uid === $uid",
        ".validate": "newData.hasChildren(['playerName', 'score', 'playCount']) && newData.child('score').isNumber()"
      }
    },
    "bug_reports": {
      ".read": "false",
      ".write": "true"
    }
  }
}